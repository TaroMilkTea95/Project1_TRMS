<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Project1_TRMS</display-name>
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.trms.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>tuitionReimbursementFormServlet</servlet-name>
    <servlet-class>com.trms.servlets.TuitionReimbursementFormServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>tuitionReimbursementFormServlet</servlet-name>
    <url-pattern>/tuitionReimbursementForm</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logoutServlet</servlet-name>
    <servlet-class>com.trms.servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>homeServlet</servlet-name>
    <servlet-class>com.trms.servlets.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>homeServlet</servlet-name>
    <url-pattern>/homeServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>approveServlet</servlet-name>
    <servlet-class>com.trms.servlets.ApproveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>approveServlet</servlet-name>
    <url-pattern>/approveServlet</url-pattern>
  </servlet-mapping>
</web-app>